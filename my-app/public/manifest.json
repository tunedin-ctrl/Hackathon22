{
  "short_name": "React App",
  "name": "Create React App Sample",
  "manifest_version": 2,
  "version": "1.0",
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "run app"
  },
  "background": {
    "scripts": ["./jquery.js", "background.js"]
  },
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "all_frames": true,
    "js": [
      "./content.js",
      "./jquery.js"
    ],
    "run_at": "document_end"
  }],
  "commands": {
    "log-search": {
      "suggested_key": {
        "default": "Ctrl+Shift+F",
        "mac": "Command+Shift+Y"
      },
      "description": "Run \"foo\" on the current page."
  },
  "run_at": "document_end"
  }
}
